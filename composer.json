{
    "name": "hiromi2424/api",
    "description": "API components for CakePHP",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "api"],
    "homepage": "https://github.com/hiromi2424/api",
    "license": "MIT",
    "authors": [
        {
            "name": "hiromi2424",
            "email": "hiromi2424@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/hiromi2424/api/issues",
        "source": "https://github.com/hiromi2424/api"
    },
    "require": {
        "cakephp/cakephp": ">=3.0.0 <4.0"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "dev-master",
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "psr-4": {
            "Api\\": "src",
            "Api\\Test\\Fixture\\": "tests\\Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests",
            "Api\\Test\\": "tests"
        }
    },
    "minimum-stability": "beta"
}